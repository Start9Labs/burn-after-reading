{
  "name": "burn-after-reading",
  "version": "0.1.0",
  "description": "A simple, fast, standalone service for sharing private information which is destroyed after viewed. Simply type your message, choose your self-destruction rules, and share the ephemeral link over Tor. Content is stored directly on the Embassy, and there are no trusted 3rd parties.",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --output-path dist",
    "build-prod": "ng build --prod --output-path dist",
    "lint": "ng lint"
  },
