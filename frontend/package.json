    "@angular/router": "^11.0.0",
    "@ionic/angular": "^5.0.0",
    "@types/mime-types": "^2.1.0",
    "@types/streamsaver": "^2.0.0",
    "@types/urlencode": "^1.1.2",
    "core-js": "^3.6.5",
    "js-base64": "^3.6.0",
    "mime": "^2.4.6",
    "mime-types": "^2.1.27",
    "rxjs": "^6.6.0",
    "streamsaver": "^2.0.5",
